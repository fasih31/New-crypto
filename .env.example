
# CryptoVerse Hub - Environment Configuration
# Copy this file to .env and fill in your values

# WalletConnect Project ID (Required for wallet connections)
# Get yours at: https://cloud.walletconnect.com
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Token Contract Addresses (Optional - uses demo mode if not set)
# Replace with your actual deployed contract addresses
VITE_CVERSE_TOKEN_ADDRESS=
VITE_USDC_TOKEN_ADDRESS=

# Security Configuration (Optional - uses defaults if not set)
VITE_MAX_SLIPPAGE=5.0
VITE_TRANSACTION_TIMEOUT=300000
VITE_RATE_LIMIT_PER_MINUTE=10
